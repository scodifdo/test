   <extension
         point="com.softmodeler.ui.editorSupport">
      <editorSupport
            filter="Document"
            id="ch.scodi.ui.editorSupport.document"
            supportClass="ch.scodi.ui.editors.support.DocumentEditorSupport">
         <editor
               id="com.softmodeler.ui.editors.UIObjectRefEditor">
         </editor>
         <editor
               id="com.softmodeler.ui.editors.AuditEditor">
         </editor>
      </editorSupport>
