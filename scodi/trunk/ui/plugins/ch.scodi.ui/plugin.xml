   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="ch.scodi.ui.handler.NetviewerO2OHandler"
            id="ch.scodi.ui.command.openO2O"
            name="%command.startO2O">
      </command>
      <command
            defaultHandler="ch.scodi.ui.handler.NetviewerO2MHandler"
            id="ch.scodi.ui.command.openO2M"
            name="%command.startO2M">
      </command>
      <command
            defaultHandler="ch.scodi.ui.handler.NewDocumentHandler"
            id="ch.scodi.ui.document.command.newDocument"
            name="%command.newDocument">
         <commandParameter
               id="classifier"
               name="%commandParameter.classifier"
               optional="false">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="ch.scodi.ui.newWizard">
      <newWizard
            class="ch.scodi.ui.wizards.NewDataObjectWizard"
            filter="MasterData">
