   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="ch.scodi.ui.properties.DocumentPropertyPage"
            icon="platform:/plugin/ch.scodi.model/icons/full/obj16/HTMLDocument.png"
            id="ch.scodi.document.html.page"
            name="%page.htmlDocument">
         <enabledWhen>
            <instanceof
                  value="ch.scodi.model.HTMLDocument">
            </instanceof>
         </enabledWhen>
      </page>
      <page
            class="ch.scodi.ui.properties.ReportDocumentPropertyPage"
            icon="platform:/plugin/ch.scodi.model/icons/full/obj16/ReportDocument.png"
            id="ch.scodi.document.report.page"
            name="%page.reportDocument">
